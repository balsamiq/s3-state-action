  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "aws-sdk": "^2.844.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.28",
    "@vercel/ncc": "^0.27.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
